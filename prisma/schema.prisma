generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Cita {
  id        Int      @id @default(autoincrement())
  tipo      String
  fecha     DateTime
  hora      String
  nombre    String
  telefono  String
  email     String
  notas     String?
  createdAt DateTime @default(now())
}
